    "mobx-react": "^4.0.3",
    "mobx-utils": "^1.1.2",
    "moment": "^2.13.0",
    "normalizr": "^2.2.1",
    "prelude-ls": "^1.1.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^5.0.3",
