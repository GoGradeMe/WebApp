{
  "name": "GoGradeWeb",
  "version": "0.1.0",
  "description": "Lanciv Gradebook app",
  "repository": {
    "type": "git"
  },
  "engines": {
    "node": "12"
  },
  "scripts": {
    "start": "parcel ./public/index.html",
    "build:webpack": "NODE_ENV=production webpack --config config/webpack.config.prod.js",
    "build": "node scripts/build.js",
    "schema:download": "apollo schema:download --endpoint=http://localhost:5000/query graphql-schema.json",
    "graphql:types": "apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --includes='src/**/*.tsx' --addTypename --globalTypesFile=src/types/graphql-global-types.ts types"
  },
  "author": "Matt Aitchison",
  "devDependencies": {
    "@auth0/auth0-spa-js": "^1.5.0",
