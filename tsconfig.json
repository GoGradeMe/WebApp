{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": false,
    "module": "commonjs",
    "target": "es6",
    "allowJs": false,
    "jsx": "react",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["dom", "es6", "dom.iterable", "scripthost", "esnext.asynciterable"],
    "typeRoots": ["./types", "./node_modules/@types"],
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "src/semantic"]
}
